# 交易服务器的代理
TradeApiFactoryPrx = TraderServer:default -p 10000 -h 115.28.79.223

# 客户端适配器
ClientAdapter.Endpoints = default

#线程池
Ice.ThreadPool.Client.Size=10
Ice.ThreadPool.Server.Size=10

Ice.ThreadPool.Client.SizeMax=100
Ice.ThreadPool.Server.SizeMax=100

Ice.ThreadPool.Client.SizeWarn=80
Ice.ThreadPool.Server.SizeWarn=80

#
# 线程栈
#
Ice.ThreadPool.Client.StackSize = 262144
Ice.ThreadPool.Server.StackSize = 262144

# 缓冲区
Ice.MessageSizeMax=65536
